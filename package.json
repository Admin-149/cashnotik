{
  "name": "cashnotik",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "db:start": "docker run --rm --name pg-docker --env-file .env -d -p 5432:5432 -v $(pwd)/../tmp/db/cashnotik:/var/lib/postgresql/data postgres",
    "db:start-volume": "docker run --rm --name pg-docker --env-file .env -d -p 5432:5432 postgres",
    "db:stop": "docker stop pg-docker",
    "prebuild": "rimraf dist",
    "build": "tsc -b",
    "build:client": "webpack",
    "build:client:dev": "webpack --watch",
    "build:client:server": "webpack-dev-server",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "nginx": "docker build -t 1.0.0 . && docker run --rm -p 80:80 -p 443:443 -v `pwd`:/www/ --name nginx-cashnotik 1.0.0",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typings:server": "ts-node src/server/generate-typings",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@loadable/component": "^5.12.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.4.6",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.11",
    "@nestjs/typeorm": "^7.1.0",
    "@reduxjs/toolkit": "^1.4.0",
    "apollo-server-express": "^2.15.0",
    "argon2": "^0.26.2",
    "emotion-theming": "^10.0.27",
    "graphql": "^15.2.0",
    "graphql-tools": "^6.0.11",
    "i18next": "^19.5.1",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-http-backend": "^1.0.15",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/loadable__component": "^5.10.0",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.39",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/supertest": "^2.0.8",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.3.0",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "source-map-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
